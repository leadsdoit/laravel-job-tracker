{
  "name": "azirka/job-tracker",
  "description": "A Laravel package to monitor jobs by groups, track their execution status, and dispatch events on group start and completion.",
  "keywords": [
    "laravel",
    "job",
    "tracker",
    "queue",
    "monitor",
    "events"
  ],
  "type": "library",
  "require": {
    "php": "^8.3 || ^8.4",
    "illuminate/support": "^11.0",
    "illuminate/database": "^11.0",
    "illuminate/queue": "^11.0",
    "illuminate/console": "^11.0",
    "illuminate/contracts": "^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9",
    "phpunit/phpunit": "^11.0"
  },
  "license": "MIT",
  "autoload": {
    "files": [
      "src/Helpers/db.php"
    ],
    "psr-4": {
      "AZirka\\JobTracker\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AZirka\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Artem Zirka",
      "email": "zirkaartem@gmail.com",
      "role": "Developer"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "AZirka\\JobTracker\\JobTrackerServiceProvider"
      ]
    }
  },
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
